@model Wedding
@{
    ViewData["Title"] = "Add a Wedding";
}
<div class="header">
    <h3>Plan your Wedding</h3> <a href="/dashboard">Dashboard</a> <a href="/logout">Log Out</a>
</div>
<div class="add">
    <form asp-action="CreateWedding" asp-controller="Wedding" class="form-group">
        <div class="form-group">
            <label asp-for="WedderOne"></label><br>
            <input asp-for="WedderOne"><br>
            <span class="text-danger" asp-validation-for="WedderOne"></span>
        </div>
        <div class="form-group">
            <label asp-for="WedderTow"></label><br>
            <input asp-for="WedderTow"><br>
            <span class="text-danger" asp-validation-for="WedderTow"></span>
        </div>
        <div class="form-group">
            <label asp-for="Date"></label><br>
            <input asp-for="Date"><br>
            <span class="text-danger" asp-validation-for="Date"></span>
        </div>
        <div class="form-group">
            <label asp-for="Address"></label><br>
            <input asp-for="Address"><br>
            <span class="text-danger" asp-validation-for="Address"></span>
        </div>
        <input type="submit" value="Create" class="btn btn-dark">
    </form>
</div>